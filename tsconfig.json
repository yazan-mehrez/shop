{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "core-js/es6/*": [
        "node_modules/core-js/es/*"
      ],
      "core-js/es7/*": [
        "node_modules/core-js/es/*"
      ]
    },
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
